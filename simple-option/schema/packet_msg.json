{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PacketMsg",
  "description": "Messages that will be sent over the IBC channel",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "propose"
      ],
      "properties": {
        "propose": {
          "type": "object",
          "required": [
            "msg",
            "tx_id"
          ],
          "properties": {
            "msg": {
              "$ref": "#/definitions/ExecuteMsg"
            },
            "tx_id": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "commit"
      ],
      "properties": {
        "commit": {
          "type": "object",
          "required": [
            "msg",
            "tx_id"
          ],
          "properties": {
            "msg": {
              "$ref": "#/definitions/ExecuteMsg"
            },
            "tx_id": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "who_am_i"
      ],
      "properties": {
        "who_am_i": {
          "type": "object"
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "ExecuteMsg": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "set"
          ],
          "properties": {
            "set": {
              "type": "object",
              "required": [
                "key",
                "value"
              ],
              "properties": {
                "key": {
                  "type": "string"
                },
                "value": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "get"
          ],
          "properties": {
            "get": {
              "type": "object",
              "required": [
                "key"
              ],
              "properties": {
                "key": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        }
      ]
    }
  }
}
